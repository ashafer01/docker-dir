# Add locations of default Kerberos servers to /etc/krb5.conf?
krb5-config	krb5-config/add_servers	boolean	false
krb5-config	krb5-config/kerberos_servers	string	127.0.0.1
krb5-config	krb5-config/default_realm	string	MY.DIR
krb5-config	krb5-config/read_conf	boolean	true
krb5-config	krb5-config/admin_server	string	127.0.0.1
krb5-config	krb5-config/add_servers_realm	string	MY.DIR
