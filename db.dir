$TTL	604800
@	IN	SOA	dir. root.dir. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dir.
@	IN	A	127.0.0.3
my      IN      A       127.0.0.3
_kerberos._udp         IN SRV 0 0  88 my
_kerberos-master._udp  IN SRV 0 0  88 my
_kerberos-adm._tcp     IN SRV 0 0 749 my
_kpasswd._udp          IN SRV 0 0 464 my
